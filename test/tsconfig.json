{
   "compilerOptions": {
      "allowUnreachableCode": false,
      "alwaysStrict": true,
      "baseUrl": "src",
      "forceConsistentCasingInFileNames": true,
      "lib": ["ES2020", "DOM"],
      "module": "ES2020",
      "moduleResolution": "node",
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "noImplicitOverride": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "outDir": "tempBuild",
      "paths": {
         "function-curve-editor": ["../../dist/FunctionCurveEditor.d.ts"]
      },
      "removeComments": true,
      "strictFunctionTypes": true,
      "strictNullChecks": true,
      "target": "ES2020"
   },
   "include": ["src"]
}
